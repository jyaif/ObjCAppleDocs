{
  "background": {
      "persistent": true,
      "scripts": ["script/background.js"]
  },
  "name": "Objective-C Apple Documentation",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Toggle Apple Documentation to default to Objective-C",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "96": "icon96.png",
    "128": "icon128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://developer.apple.com/reference/*"
  ],
  "browser_action": {
    "default_icon": {
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "Toggle Obj-C Documentation"
  }
}
